{
  "pluginName": "iTerm执行命令",
  "description": "打开iTerm并执行命令",
  "author": "yhan219",
  "homepage": "https://blog.yhan219.com",
  "logo": "logo.png",
  "version": "0.0.3",
  "preload": "preload.js",
  "platform": [
    "darwin"
  ],
  "features": [
    {
      "code": "iterm_command",
      "explain": "打开iterm并执行命令",
      "cmds": [
        "iterm",
        ">",
        {
          "type": "over",
          "label": "打开iTerm并执行",
          "minLength": 2,
          "maxLength": 50
        }
      ]
    },
    {
      "code": "current_iterm_command",
      "explain": "当前目录打开iTerm并执行",
      "cmds": [
        "iterm",
        ".>",
        {
          "type": "over",
          "label": "当前目录打开iTerm并执行",
          "minLength": 2,
          "maxLength": 50
        }
      ]
    }
  ]
}